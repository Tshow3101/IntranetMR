    <style>
        .spinner, .spinner-small {
            height: 40px;
            width: 40px;
            position: absolute;
            top: 50%;
            left: 50%;
            margin: -20px 0 0 -20px;
            border: 2px solid #fff;
            border-top: 2px solid #db0281;
            border-radius: 100%;
        }

        .pace-progress {
            background: #db0281;
        }

        .pace-progress, .pace:before {
            top: 100px;
            right: 0;
            height: 3px;
            position: fixed;
            left: 0;
        }

        .page-header-fixed {
            padding-top: 46px;
        }

        input, select, textarea {
            max-width: 100%;
        }
    </style>

